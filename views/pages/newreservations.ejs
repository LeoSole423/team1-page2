<form id="formLogin" method="POST" action="/newreservations" enctype="multipart/form-data"
      class="d-flex flex-column text-center align-items-center py-2">

    <div class="form-group w-25">
        <label for="initialDate"><b>Dia de entrada</b></label>
        <input type="text" class="form-control" id="initialDate" name="initialDate">
    </div>

    <div class="form-group w-25">
        <label for="finalDate"><b>Dia de salida</b></label>
        <input type="text" class="form-control" id="finalDate" name="finalDate">
    </div>

    <div class="form-group w-25">
        <label for="paymentMethod"><b>Metodo de pago</b></label>
        <input type="text" class="form-control" id="paymentMethod" name="paymentMethod">
    </div>


    <div class="form-group w-25">
        <label for="valuePerDay"><b>Valor diario</b></label>
        <input type="number" class="form-control" id="valuePerDay" name="valuePerDay">
    </div>


    <div class="form-group w-25">
        <label for="totalValue"><b>Valor total</b></label>
        <input type="number" class="form-control" id="totalValue" name="totalValue">
    </div>


    <div class="form-group w-25">
        <label for="downPayment"><b>Deposito</b></label>
        <input type="number" class="form-control" id="downPayment" name="downPayment">
    </div>


    <button type="submit" class="btn-lg btn-success w-25">Crear</button>
